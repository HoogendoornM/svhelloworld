{
    "name": "sv-helloworld/mijn-sv-helloworld",
    "keywords": ["sv", "hello", "world"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-dompdf": "^0.8",
        "caouecs/laravel-lang": "~3.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.4",
        "jrean/laravel-user-verification": "^8.0",
        "laracasts/flash": "^3.0",
        "laravel/dusk": "^5.6",
        "laravel/framework": "^6.2",
        "laravel/helpers": "^1.1",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.0",
        "lavary/laravel-menu": "^1.7.7",
        "mollie/laravel-mollie": "^2.8",
        "sentry/sentry-laravel": "^1.5",
        "spatie/laravel-newsletter": "^4.7",
        "thomaswelton/laravel-gravatar": "^1.2"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-pcntl": "7.2",
            "ext-posix": "7.2"
        }
    }
}
